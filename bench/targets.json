[
  {
    "name": "custom-next-langchain",
    "baseUrl": "http://localhost:3000",
    "fetchRegex": "/\\/api\\/(chat|nlq)/",
    "selectors": {
      "input": "input[type='text']",
      "send": "button[type='submit']",
      "assistant": "div[style*='overflow-y'] > div > div"
    }
  },
  {
    "name": "copilotkit-ui",
    "baseUrl": "http://localhost:3003",
    "fetchRegex": "/\\/api\\/(chat|nlq)/",
    "selectors": {
      "input": "textarea[placeholder*='Type a message']",
      "send": "button[data-test-id='copilot-chat-ready']",
      "assistant": "[role='status']"
    }
  },
  {
    "name": "agent-chat-ui-langgraph",
    "baseUrl": "http://localhost:3001",
    "fetchRegex": "/\\/api\\/(threads|runs|chat)/",
    "selectors": {
      "input": "textarea[placeholder*='Type your message']",
      "send": "button[type='submit']",
      "assistant": "[data-role='assistant']"
    }
  }
]
